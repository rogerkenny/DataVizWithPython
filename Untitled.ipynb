    "a = [1,2,3,4,5]\n",
    "\n",
    "def rotLeft(a, d):\n",
    "    temp = [0]*len(a)\n",
    "    i = 0\n",
    "    for e in a:\n",
    "        newIdx = (i-d)%len(a)\n",
    "        temp[newIdx] = e\n",
    "        i += 1\n",
    "    print(temp)\n",
    "    \n",
    "rotLeft(a, 1)"
   ]
